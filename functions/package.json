{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "type": "module",
  "scripts": {
    "serve": "NODE_ENV=development firebase emulators:start --import=./.emulator-data | tee functions-emulator.log",
    "serve:clean": "NODE_ENV=development firebase emulators:start > functions-emulator.log",
    "serve:with_export": "npm run serve -- --export-on-exit=./.emulator-data",
    "shell": "NODE_ENV=development firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "runtime": "which firebase"
  },
  "engines": {
    "node": "20"
  },
  "main": "index.js",
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.9.0"
  },
  "devDependencies": {},
  "private": true
}